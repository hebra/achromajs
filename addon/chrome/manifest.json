{
	"manifest_version": 2,
	"name": "AchromaJS",
	"description": "A browser extension to simulate different conditions of eye vision for the current web page.",
	"version": "1.0",
	"icons": 
	{
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	},

	"browser_action": 
	{
		"default_icon": "img/icon-19.png",
		"default_popup": "popup.html"
	},

	"permissions": 
	[
		"activeTab"
	]
}